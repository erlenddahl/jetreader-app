                 xmlns:pages="clr-namespace:Rg.Plugins.Popup.Pages;assembly=Rg.Plugins.Popup"
                 xmlns:popups="clr-namespace:JetReader.Page.Reader.Popups;assembly=JetReaderLibrary"
                 x:Class="JetReader.Page.Reader.Popups.ThemePickerPopup">
    <StackLayout VerticalOptions="Center" Margin="20">
        <ScrollView>
            <FlexLayout x:Name="Container" AlignItems="Center" Wrap="Wrap" JustifyContent="Center">
            </FlexLayout>
        </ScrollView>
    </StackLayout>
</pages:PopupPage>